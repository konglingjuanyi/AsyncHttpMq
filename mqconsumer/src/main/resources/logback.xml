<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%date %5p | %-10logger{10} | %m %n</pattern>
		</encoder>
	</appender>
	<logger name="org.springframework">
		<level value="debug" />
		<appender-ref ref="CONSOLE" />
	</logger>
	<root>
		<level value="debug" />
	</root>
</configuration>